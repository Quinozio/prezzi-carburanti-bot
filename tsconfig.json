{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["ES2021"],
    "allowJs": true,
    "skipLibCheck": true,
    "outDir": "build",
    "rootDir": "src",
    "strict": false,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}
